							<ProtectedString name="Source"><![CDATA[local Frame = script.Parent.Parent
local Emote = script.Parent.Parent.Frame2
local Button = script.Parent
local ui = Emote.UIStroke
local player = game.Players.LocalPlayer

function Ouvrir(button)
	if ui.color == Color3.fromRGB(60, 255, 0) then 
		Frame.Visible = false
		game.Lighting.Blur.Size = 0
	end
	if ui.color == Color3.fromRGB(255, 255, 255) then 
		Frame.Error.Visible = true
		wait(2)
		Frame.Error.Visible = false
	end
end

Button.MouseButton1Click:Connect(Ouvrir)

]]></ProtectedString>
