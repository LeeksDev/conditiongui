					<ProtectedString name="Source"><![CDATA[-- Créez une variable booléenne pour stocker l'état de la première connexion du joueur
local playerHasJoinedBefore = false

-- Créez une fonction pour afficher le ScreenGui lorsque le joueur rejoint pour la première fois
local function showScreenGuiOnFirstJoin()
	-- Vérifiez si le joueur a déjà rejoint avant
	if not playerHasJoinedBefore then
		-- Si c'est la première fois que le joueur rejoint, affichez le ScreenGui
		local screenGui = script.Parent:WaitForChild("TermandCondition")
		screenGui.Enabled = true
		-- Modifiez la variable pour indiquer que le joueur a déjà rejoint
		playerHasJoinedBefore = true
	end
end

-- Connectez la fonction à l'événement PlayerAdded de la propriété Players
game.Players.PlayerAdded:Connect(showScreenGuiOnFirstJoin)
]]></ProtectedString>
